    "version": "0.2.0",
    "configurations": [
        {
            "type": "chrome",
            "request": "launch",
            "name": "Launch Chrome",
            "url": "http://localhost:80",
            "webRoot": "${workspaceFolder}/ProdFrontend/"
        },
       


        {
            "type": "node",
            "request": "attach",
            "name": "Attach",
            "port": 9229,
            "restart": true,
            "localRoot": "${workspaceRoot}/ProdBackend/",
            "remoteRoot": "/app/",
            "protocol": "inspector"
        },
    ]
}